{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://inventory-management-minimal.vercel.app/api/:path*"
        },
        {
            "source": "/api/:path*/",
            "destination": "https://inventory-management-minimal.vercel.app/api/:path*"
        },
        {
            "source": "/:path*",
            "destination": "/"
        }
    ]
}